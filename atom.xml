---
layout: nil
permalink: /atom_feed.xml
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>OpenYou.org</title>
 <link href="http://www.openyou.org/atom.xml" rel="self"/>
 <link href="http://www.openyou.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.openyou.org/</id>
 <author>
   <name>Kyle Machulis</name>
   <email>kyle at nonpolynomial dot com</email>
 </author>

 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.openyou.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.openyou.org{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
